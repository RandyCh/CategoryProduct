@model CategoryProduct.Models.Categories
@if(Model.Picture!=null){
<img style="width:320px;height:240px;" src="@Url.Action("ShowPhoto", "Categories", new {id=Model.CategoryID})" title="@Html.DisplayFor(model=>model.CategoryName)" />
}
else{
<img style="width:320px;height:240px;" src="~/image/noimage.png" />
}
